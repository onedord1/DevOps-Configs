apiVersion: operator.tigera.io/v1
kind: Installation
metadata:
  name: default
spec:
  calicoNetwork:
    ipPools:
    - blockSize: 26
      cidr: "{{ pod_network_cidr }}"
      encapsulation: VXLANCrossSubnet
      natOutgoing: {{ calico_nat_outgoing }}
      nodeSelector: all()
---
apiVersion: operator.tigera.io/v1
kind: APIServer
metadata:
  name: default
spec: {}