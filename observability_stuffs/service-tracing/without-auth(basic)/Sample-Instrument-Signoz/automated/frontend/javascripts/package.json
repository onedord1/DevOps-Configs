{
  "name": "aesl-skeleton",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepare": "husky",
    "up": "docker compose up -d",
    "start": "docker compose up --build -d",
    "stop": "docker compose stop",
    "publish": "docker compose -f docker compose.deploy.yml build --no-cache && docker compose -f docker compose.deploy.yml push"
  },
  "dependencies": {
    "@f3ve/vue-markdown-it": "^0.2.3",
    "@observablehq/plot": "^0.6.16",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.1",
    "@opentelemetry/auto-instrumentations-web": "^0.49.0",
    "@opentelemetry/context-zone": "^2.0.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-web": "^2.0.1",
    "@pinia/testing": "^1.0.1",
    "@vueuse/core": "^10.11.1",
    "@vueuse/integrations": "^11.2.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.6.5",
    "chartjs-adapter-date-fns": "^3.0.0",
    "downloadjs": "^1.4.7",
    "flag-icons": "^7.2.3",
    "jspdf": "^2.5.1",
    "lottie-web": "^5.12.2",
    "nats.ws": "^1.30.3",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.2",
    "posthog-js": "^1.257.0",
    "universal-cookie": "^7.2.2",
    "v-onboarding": "^2.9.0",
    "v-phone-input": "^4.2.1",
    "vue": "^3.5.12",
    "vue-chartjs": "^5.3.1",
    "vue-router": "^4.4.5",
    "vue-toast-notification": "^3.1.3",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-toastify": "^0.2.3",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.9.0"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.64",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/downloadjs": "^1.4.6",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.0",
    "cypress": "^13.6.1",
    "eslint": "^8.49.0",
    "eslint-plugin-custom": "file:eslint-plugin-custom",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jsdoc": "^50.5.0",
    "eslint-plugin-json-files": "^4.4.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.29.1",
    "husky": "^9.1.6",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.3.3",
    "prettier-plugin-vue": "^1.1.6",
    "sass-embedded": "^1.80.3",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.3.0",
    "unplugin-icons": "^0.18.2",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.5.3",
    "vite-plugin-vuetify": "^2.0.4",
    "vitepress": "^1.5.0",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.1.6"
  },
  "lint-staged": {
    "*.{js,ts,vue,md,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
