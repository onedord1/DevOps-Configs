{
  "name": "aes-erp-scm",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "husky",
    "sonar": "sonar-scanner"
  },
  "dependencies": {
    "@observablehq/plot": "^0.6.16",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.1",
    "@opentelemetry/auto-instrumentations-web": "^0.49.0",
    "@opentelemetry/context-zone": "^2.0.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-web": "^2.0.1",
    "@vueuse/core": "^10.7.2",
    "@vueuse/integrations": "^10.11.1",
    "axios": "^1.6.5",
    "date-fns": "^4.1.0",
    "downloadjs": "^1.4.7",
    "flag-icons": "^7.2.3",
    "keycloak-js": "^25.0.2",
    "pinia": "^2.2.4",
    "v-phone-input": "^4.2.1",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vue-toastification": "^2.0.0-rc.5",
    "vuetify": "^3.7.3"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.64",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/downloadjs": "^1.4.6",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "cypress": "^13.6.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.49.0",
    "eslint-plugin-custom": "file:eslint-plugin-custom",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jsdoc": "^50.5.0",
    "eslint-plugin-json-files": "^4.4.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.29.1",
    "husky": "^9.1.6",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.3.3",
    "prettier-plugin-vue": "^1.1.6",
    "sass-embedded": "^1.80.3",
    "sonarqube-scanner": "^4.2.3",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.3.0",
    "unplugin-icons": "^0.18.2",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.5.3",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
  },
  "lint-staged": {
    "*.{js,ts,vue,css,sass,md,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
